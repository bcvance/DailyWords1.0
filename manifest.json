{
    "name": "Daily Words",
    "description": "Extension that sends you daily word definitions based on words you encounter online.",
    "version": "0.0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jquery.js", "dist/main.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    }
}